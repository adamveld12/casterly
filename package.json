{
  "name": "casterly",
  "version": "0.0.1",
  "description": "A podcast web app",
  "main": "index.js",
  "scripts": {
    "docker-dev": "webpack-dev-server --config webpack.config.docker.js --watch --progress --colors --hot --inline -d --host 0.0.0.0 --port 80",
    "devbuild": "webpack --progress --display-modules --colors -d",
    "build": "webpack --progress --display-modules --colors -p",
    "dev": "docker build -f ./Dockerfile.dev -t casterly-dev . && docker run -v $PWD:/usr/src/app -p 80:80 casterly-dev",
    "live": "webpack-dev-server --progress --colors --watch -p --content-base ./public/",
    "test_deploy": "webpack --progress --display-modules --colors -p && surge -d casterly.surge.sh -p ./public",
    "deploy": "webpack --progress --display-modules --colors -p && surge -d podcast.veldhousen.ninja -p ./public"
  },
  "keywords": [
    "podcast",
    "streaming"
  ],
  "author": "Adam Veldhousen",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webworker-loader": "^0.1.0",
    "worker-loader": "^0.7.0"
  },
  "dependencies": {
    "fbjs": "^0.7.2",
    "immutable": "^3.7.6",
    "jquery": "^2.1.4",
    "localforage": "^1.2.10",
    "material-ui": "^0.14.4",
    "mcfly": "0.0.10",
    "moment": "^2.10.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.0-rc1",
    "react-fontawesome": "^0.3.3",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "xml2js": "^0.4.12"
  }
}
